{
  "name": "backend",
  "version": "0.2.0",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "start:dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "build": "tsc",
    "migration:run": "node ../node_modules/typeorm/cli-ts-node-commonjs.js migration:run -d src/data-source.ts",
    "migration:generate": "typeorm migration:generate src/migrations",
    "migration:create": "typeorm migration:create src/migrations"
  },
  "dependencies": {
    "@nestjs/bull": "^10.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.4.22",
    "@nestjs/platform-socket.io": "^10.0.0",
    "@nestjs/schedule": "^6.1.1",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.0.0",
    "axios": "^1.13.5",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.6.2",
    "bull": "^4.16.5",
    "bullmq": "^1.80.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "eventemitter3": "^4.0.7",
    "fast-xml-parser": "^5.3.5",
    "mssql": "^12.2.0",
    "multer": "^2.0.2",
    "nodemailer": "^6.9.16",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.18.0",
    "redis": "^5.10.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "sqlite3": "^5.1.7",
    "tedious": "^19.2.0",
    "typeorm": "^0.3.28"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
